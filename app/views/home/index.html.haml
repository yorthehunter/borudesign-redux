.content.signal
  .hd
  .container
    .row
      .span6
        .logo
          %img{ src: asset_path("logo-signal.png") }
      .span6
        %h1
          Signal
          %small
            %a{href:"http://www.signaldetected.com",target:"_blank"}
              %i.icon-external-link
        %h3 Social networking iPhone App
        %p Stay connected to your inner circle of trusted professional contacts. Signal is a mix of Path and LinkedIn, allowing you to curate your professional contacts.
    #sig-container.modal-container
      .row.padded
        .span3
          .thumbnail
            %a(href="#sig" data-toggle="modal")
              %img{src:asset_path("img-sm-sig-4.jpg")}
        .span3
          .thumbnail
            %a(href="#sig" data-toggle="modal")
              %img{src:asset_path("img-sm-sig-6.jpg")}
        .span3
          .thumbnail
            %a(href="#sig" data-toggle="modal")
              %img{src:asset_path("img-sm-sig-1.jpg")}
        .span3
          .thumbnail
            %a(href="#sig" data-toggle="modal")
              %img{src:asset_path("img-sm-sig-3.jpg")}
  .ft

